{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "host", "value": "arpitsolar.com" }
      ],
      "destination": "https://www.arpitsolar.com/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
